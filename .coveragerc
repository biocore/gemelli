# this file is based on the examples provided on scikit-learn's .coveragerc

[run]
omit =
    */tests*
    */__init__.py
source = gemelli
branch = True
include = */gemelli/*

[report]
show_missing = True
exclude_lines =
    pragma: no cover
    raise NotImplementedError
    if __name__ == .__main__.:
omit =
    */tests*
    */__init__.py
    */base.py
